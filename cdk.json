{
  "app": "npx ts-node --prefer-ts-exts app.ts",
  "watch": {
    "include": ["**"],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json",
      "node_modules"
    ]
  },
  "context": {
    "aws-cdk:enableDiffNoFail": true
  }
}
